div.wp-tiles-tile:hover:before {
  background:url('../images/gallery-ho.png') no-repeat center center;
  border-radius: 100%;
  content: "";
  display: block;
  height: 60px;
  left: 50%;
  margin-left: -30px;
  margin-top: -30px;
  overflow: auto;
  position: absolute;
  top: 50%;
  width: 60px;
  z-index: 2;
  cursor: pointer;
}

