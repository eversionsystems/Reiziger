.clearfix:after, .grid-block:after, .deepest:after {
    clear: both;}
.clearfix:before, .clearfix:after, .grid-block:before, .grid-block:after, .deepest:before, .deepest:after {
    content: "";
    display: table;
}

article, aside, figure, footer, header, hgroup,
